FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (0 0 0);

boundaryField
{
    top
    {
        type            fixedValue;
        uniform         (0 0 0); // to be adapted for rotation of top disk
    }

    bottom
    {
        type            fixedValue;
        uniform         (0 0 0);
    }

    front
    {
        type            empty;
    }

    back
    {
        type            fixedValue;
        uniform         (0 0 0);
    }

    left
    {
        type            wedge;
    }

    right
    {
        type            empty;
    }
}

// ************************************************************************* //
