FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      faSolution;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

solvers
{
    CsTrans
    {
        solver          PBiCG;
        preconditioner  DILU;
        minIter         4;
        maxIter         2000;
        tolerance       1e-18;
        relTol          0;
    }

    CsCis
    {
        solver          PBiCG;
        preconditioner  DILU;
        minIter         4;
        maxIter         2000;
        tolerance       1e-18;
        relTol          0;
    }
}

