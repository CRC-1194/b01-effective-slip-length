FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      sample;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

type sets;
libs            (sampling);

interpolationScheme cell;

setFormat       csv;

sets
(
    data
    {
        type    uniform;
        axis    x;
        start   (0 5e-4 0);
        end     (0.001 5e-4 0);
        nPoints 100;
    }
);

fields          (U mag(U));

