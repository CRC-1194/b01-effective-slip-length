FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      controlDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //


applicationClass    surfaceTrackingFoam;

startFrom           startTime;

startTime           0;

stopAt              endTime;

endTime             1e-1;

deltaT              1e-08;

writeControl        timeStep; //adjustableRunTime;  //runTime;

writeInterval       1;

cycleWrite          0;

writeFormat         ascii;

writePrecision      10;

writeCompression    uncompressed;

timeFormat          general;

timePrecision       10;

runTimeModifiable   yes;

adjustTimeStep      no;

