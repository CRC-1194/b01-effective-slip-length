FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      controlDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

application     pimpleFoam;

startFrom       startTime;

startTime       0;

stopAt          endTime;

endTime         0.1;

deltaT          5e-06;

writeControl    timeStep;

writeInterval   1000;

purgeWrite      0;

writeFormat     binary;

writePrecision  6;

writeCompression off;

timeFormat      general;

timePrecision   6;

runTimeModifiable true;

adjustTimeStep  no;

maxCo           0.2;

functions
{
    sampleLine
    {
        type sets;
        libs            (sampling);

        interpolationScheme cell;

        setFormat       csv;

        sets
        (
            data
            {
                type    uniform;
                axis    x;
                start   (0 5e-4 0);
                end     (0.001 5e-4 0);
                nPoints 100;
            }
        );

        fields          (U);
    }
}

// ************************************************************************* //
